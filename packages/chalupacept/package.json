{
	"name": "@chalupajs/chalupacept",
	"version": "1.6.0",
	"description": "Integration testing framework for Chalupa",
	"bin": {
		"chalupacept": "./dist/src/bin.js"
	},
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rimraf ./dist ./coverage ./generated-docs ./junit.xml",
		"compile": "tsc -p tsconfig.build.json",
		"compile:watch": "tsc -p tsconfig.build.json --watch",
		"docs": "typedoc --out generated-docs src",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"prepublishOnly": "pnpm run clean && pnpm run compile",
		"test": "jest --coverage",
		"test:watch": "jest --watch"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:chalupajs/chalupa.git"
	},
	"license": "MIT",
	"keywords": [],
	"author": "",
	"devDependencies": {
		"@dwmt/eslint-config": "1.0.0",
		"@types/chai": "4.3.0",
		"@types/glob": "7.2.0",
		"@types/mocha": "9.1.0",
		"@types/node": "16.10.3",
		"eslint": "7.32.0",
		"prettier": "2.4.1",
		"rimraf": "3.0.2",
		"typedoc": "0.21.0",
		"typescript": "4.3.3"
	},
	"engines": {
		"node": ">=16"
	},
	"dependencies": {
		"@chalupajs/interface": "workspace:*",
		"@chalupajs/service": "workspace:*",
		"@chalupajs/test-framework": "workspace:*",
		"@codeceptjs/helper": "^1.0.2",
		"chai": "4.3.6",
		"chalk": "4.1.2",
		"commander": "8.3.0",
		"dts-dom": "3.6.0",
		"faker": "5.5.1",
		"glob": "7.2.0",
		"inversify": "5.1.1",
		"mocha": "9.2.1",
		"pretty-error": "4.0.0",
		"reflect-metadata": "0.1.13",
		"ts-node": "10.2.1"
	}
}
