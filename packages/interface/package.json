{
	"name": "@chalupajs/interface",
	"version": "1.3.0",
	"description": "Service framework",
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.ts",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"clean": "rimraf ./dist ./coverage ./generated-docs ./junit.xml",
		"compile": "tsc -p tsconfig.build.json",
		"compile:watch": "tsc -p tsconfig.build.json --watch",
		"docs": "typedoc --out generated-docs src",
		"test": "jest --coverage",
		"lint": "eslint ./src",
		"lint:fix": "eslint ./src --fix",
		"prepublishOnly": "pnpm run clean && pnpm run compile"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:chalupajs/chalupa.git"
	},
	"license": "MIT",
	"devDependencies": {
		"@dwmt/eslint-config": "1.0.0",
		"@types/convict": "6.1.1",
		"@types/js-yaml": "4.0.3",
		"@types/uuid": "8.3.1",
		"eslint": "7.32.0",
		"prettier": "2.4.1",
		"rimraf": "3.0.2",
		"ts-node": "10.2.1",
		"typedoc": "0.21.0",
		"typescript": "4.3.3"
	},
	"dependencies": {
		"inversify": "5.1.1",
		"js-yaml": "4.0.0",
		"konvenient": "3.1.0",
		"reflect-metadata": "0.1.13",
		"uuid": "8.3.2"
	}
}
