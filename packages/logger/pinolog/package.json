{
	"name": "@catamaranjs/logger-pino",
	"version": "1.0.0",
	"description": "Service framework",
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.ts",
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"clean": "rimraf ./dist ./coverage ./generated-docs ./junit.xml",
		"compile": "tsc -p tsconfig.build.json",
		"compile:watch": "tsc -p tsconfig.build.json --watch",
		"docs": "typedoc --out generated-docs src",
		"lint": "xo",
		"lint:fix": "xo --fix",
		"prepublishOnly": "npm run clean && npm run compile"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:dwmt/Catamaran.git"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/pino": "6.3.11",
		"rimraf": "3.0.2",
		"typedoc": "0.21.0",
		"typescript": "4.3.3"
	},
	"dependencies": {
		"@catamaranjs/interface": "workspace:*",
		"pino": "6.13.3",
		"pino-pretty": "7.0.1",
		"reflect-metadata": "0.1.13"
	}
}
